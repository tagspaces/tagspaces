{
  "name": "tagspaces",
  "version": "0.0.0",
  "description": "TagSpaces core repository",
  "homepage": "https://www.tagspaces.org",
  "scripts": {
    "lint": "eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "yarn lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "hot-updates-server": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "build": "yarn version-meta && concurrently \"yarn build-main\" \"yarn build-renderer\" \"yarn build-worker\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --progress --profile --colors",
    "build-cordova": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.cordova.js --progress --profile --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --progress --profile --colors",
    "build-worker": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.worker.prod.js --progress --profile --colors",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-portable": "cross-env NODE_ENV=production electron ./app/ --portable",
    "prestart_bak": "yarn build",
    "flow": "flow check",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --overwrite || true",
    "start-hot-renderer": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./app/main.dev",
    "postinstall": "concurrently \"yarn flow-typed\" \"yarn build\" \"yarn build-dll\" \"install-app-deps\" \"node node_modules/fbjs-scripts/node/check-dev-engines.js package.json\"",
    "dev": "cross-env START_HOT=1 yarn hot-updates-server",
    "bump-version": "yarn third-party && cd app && yarn version && cd ..",
    "version-meta": "node scripts/versionmeta.js",
    "third-party": "yarn licenses generate-disclaimer --silent > app/third-party.txt",
    "package": "yarn bump-version && yarn build && yarn package-win && yarn package-linux && yarn package-mac",
    "package-mac": "build --mac --x64 --config scripts/builder.json",
    "package-mac-pro": "build --mac --x64 --config app/node_modules/@tagspaces/pro/resources/builder-pro.json",
    "package-win": "build --win --x64 --config scripts/builder.json",
    "package-win-pro": "build --win --x64 --config app/node_modules/@tagspaces/pro/resources/builder-pro.json",
    "package-linux": "build --linux --config scripts/builder.json",
    "package-linux-pro": "build --linux --config app/node_modules/@tagspaces/pro/resources/builder-pro.json",
    "package-pro": "patch-package-json && yarn install-ext && yarn build && yarn package-win-pro && yarn package-linux-pro && yarn package-mac-pro",
    "install-ext": "cd app && yarn install --force --prod --no-lockfile && cd ..",
    "setup-ext": "node scripts/setupextensions.js",
    "setup-pro": "node scripts/setuppro.js",
    "start-webdav": "node scripts/webdavserver.js",
    "run-web": "yarn build-cordova && yarn webdav-server",
    "run-android": "cd cordova && cordova run android && cd ..",
    "cordova-crosswalk": "cd cordova && cordova plugin add cordova-plugin-crosswalk-webview && cd ..",
    "cordova-android": "cd cordova && cordova platform add android && cd ..",
    "cordova-create": "cordova create ./cordova org.tagspaces.mobile TagSpaces",
    "start-android": "yarn build-cordova && yarn run-android"
  },
  "browserslist": "electron 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagspaces/tagspaces/"
  },
  "author": {
    "name": "TagSpaces UG (haftungsbeschraenkt)",
    "email": "info@tagspaces.org",
    "url": "https://www.tagspaces.org"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/tagspaces/tagspaces/issues"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "react",
    "redux",
    "flow",
    "sass",
    "webpack",
    "hot",
    "reload"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
