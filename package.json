{
  "name": "tagspaces",
  "version": "0.0.0",
  "description": "TagSpaces core repository",
  "homepage": "https://www.tagspaces.org",
  "scripts": {
    "bump-version": "yarn third-party && cd app && yarn version && cd .. && yarn version-meta",
    "update-i18n": "tx pull -a -d",
    "version-meta": "node scripts/versionmeta.js",
    "third-party": "yarn licenses generate-disclaimer --silent > app/third-party.txt",
    "build": "rimraf app/dist && yarn version-meta && concurrently \"yarn build-main\" \"yarn build-renderer\" \"yarn build-worker\" \"yarn build-web\"",
    "build-dll": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.renderer.dev.dll.babel.js --colors",
    "build-e2e": "cross-env E2E_BUILD=true yarn build",
    "build-main": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.main.prod.babel.js --colors",
    "build-renderer": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.renderer.prod.babel.js --colors",
    "build-worker": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.worker.prod.babel.js --colors",
    "build-worker-dev": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.worker.dev.js --colors",
    "build-web": "rimraf web/dist && cross-env NODE_ENV=production webpack --config ./configs/webpack.config.web.prod.babel.js --colors",
    "build-web-dev": "rimraf web/dist && cross-env NODE_ENV=production webpack --config ./configs/webpack.config.web.dev.babel.js --colors",
    "build-cordova": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.cordova.prod.babel.js --colors",
    "build-cordova-dev": "cross-env NODE_ENV=development webpack --config ./configs/webpack.config.cordova.dev.babel.js --colors",
    "dev": "cross-env START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 yarn start-renderer-dev",
    "lint-old": "cross-env NODE_ENV=development eslint --cache --format=pretty .",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "lint-fix": "yarn --silent lint --fix; exit 0",
    "lint-styles": "stylelint --ignore-path .eslintignore '**/*.*(css|scss)' --syntax scss",
    "lint-styles-fix": "yarn --silent lint-styles --fix; exit 0",
    "postlint-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{*{js,jsx,json},babelrc,eslintrc,prettierrc,stylelintrc}'",
    "postlint-styles-fix": "prettier --ignore-path .eslintignore --single-quote --write '**/*.{css,scss}'",
    "precommit": "lint-staged",
    "prettier-check": "prettier --check **/*.{js,json,ts,tsx,yml}",
    "prettier-format": "prettier --write **/*.{js,jsx,json,ts,tsx,yml}",
    "prestart-bak": "yarn build",
    "bs": "yarn build && yarn start",
    "start": "cross-env NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron ./app/main.dev.babel.js",
    "start-renderer-dev": "cross-env NODE_ENV=development webpack-dev-server --config configs/webpack.config.renderer.dev.babel.js",
    "type-check": "tsc --noEmit",
    "test-e2e-live": "node -r @babel/register ./internals/scripts/CheckBuiltsExist.js && cross-env NODE_ENV=test testcafe-live electron:./ ./test/e2e/HomePage.e2e.ts",
    "package": "yarn build && yarn package-win && yarn package-linux && yarn package-mac && yarn package-web",
    "package-ci": "yarn postinstall && yarn build && electron-builder --publish always",
    "package-mac": "electron-builder --mac --x64 --config resources/builder.json",
    "package-mac-pro": "electron-builder --mac --x64 --config extensions/tagspacespro/resources/builder-pro.json",
    "package-win": "electron-builder --win --x64 --config resources/builder.json",
    "package-win-pro": "electron-builder --win --x64 --config extensions/tagspacespro/resources/builder-pro.json",
    "package-linux": "electron-builder --linux --config resources/builder.json",
    "package-linux-pro": "electron-builder --linux --config extensions/tagspacespro/resources/builder-pro.json",
    "package-pro": "node scripts/enablepro.js && yarn install-ext && yarn build && yarn package-web-pro && yarn clean-maps && yarn package-win-pro && yarn package-linux-pro && yarn package-web-pro && yarn package-mac-pro",
    "package-android": "yarn build-cordova && cd cordova && npm install && cordova build --release android",
    "package-web": "yarn prepare-web && yarn clean-maps && zip -r ../builds/tagspaces-web.zip web -x web/files/**\\*",
    "package-web-pro": "yarn prepare-web-pro && yarn clean-maps && zip -r ../builds/tagspaces-pro-web.zip web -xi web/files/**\\* ",
    "prepare-web": "rimraf web/node_modules && shx cp -R app/node_modules/ web && shx cp LICENSE.txt web",
    "prepare-web-pro": "rimraf web/node_modules && shx cp -R app/node_modules/ web && shx cp extensions/tagspacespro/EULA.txt web && node scripts/genswconf.js",
    "prepare-cordova": "rimraf cordova/www/dist && rimraf cordova/www/node_modules && shx cp -rL app/node_modules cordova/www && shx cp LICENSE.txt cordova/www",
    "paste-android-linux": "adb shell input text '$(xclip -selection c -o)'",
    "paste-android-macos": "adb shell input text \"'$(pbpaste)'\"",
    "clean": "rimraf node_modules && rimraf app/dist",
    "clean-pro": "yarn clean-maps",
    "clean-maps": "rimraf app/dist/**/*.map && rimraf web/dist/**/*.map",
    "install-ext": "cd app && yarn install --force --prod --no-lockfile && cd ..",
    "setup-ext": "node scripts/setupextensions.js",
    "setup-pro": "node scripts/setuppro.js",
    "run-webdav": "yarn build-web && yarn prepare-web && node scripts/webdavserver.js",
    "run-web": "yarn build-web && yarn prepare-web && node scripts/webserver.js",
    "run-web-dev": "yarn build-web-dev && yarn prepare-web && node scripts/webserver.js",
    "run-android": "yarn prepare-cordova && yarn build-cordova && cd cordova && cordova run android && cd ..",
    "run-android-dev": "yarn prepare-cordova && yarn build-cordova-dev && cd cordova && cordova run android && cd ..",
    "run-ios-dev": "yarn prepare-cordova && yarn build-cordova-dev && cd cordova && cordova run ios && cd ..",
    "cordova-list-emulators": "cd cordova && cordova run --list --emulator && cd ..",
    "cordova-add-android": "cd cordova && cordova platform add android && cd ..",
    "cordova-create-app": "cordova create ./cordova org.tagspaces.iosn TagSpaces",
    "update-changelog": "git log v3.6.2..HEAD --no-merges --format=%B > CHANGELOG.TXT",
    "update-changelog-pro": "cd extensions/pro && git log v3.6.2..HEAD --no-merges --format=%B > ../../CHANGELOG-PRO.TXT",
    "source-map-app": "source-map-explorer app/dist/*.* --html reports/source-map-app.html",
    "source-map-web": "source-map-explorer web/dist/*.* --html reports/source-map-web.html",
    "source-map-cordova": "source-map-explorer cordova/www/dist/*.* --html reports/source-map-cordova.html",
    "source-map": "yarn source-map-app && yarn source-map-web && yarn source-map-cordova",
    "test-old": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./test/runTests.js",
    "test-watch": "yarn test --watch",
    "test-e2e-old": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./tests/runTests.js e2e",
    "test-e2e-dev": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings ./tests/runTests.js e2e-dev",
    "test-unit": "cross-env NODE_JEST=unit_test BABEL_DISABLE_CACHE=1 jest --config=jest.config.unit.js --verbose ./tests/unit/",
    "test-all": "yarn build && yarn test-unit && yarn build-e2e && yarn test-e2e",
    "test-one": "jest --detectOpenHandles --config=jest.config.web.js -t 'TST5024' --runInBand --verbose ./tests/e2e/",
    "test-e2e-electron": "node -r @babel/register ./internals/scripts/CheckBuiltsExist.js && cross-env NODE_ENV=test HEADLESS_MODE=true jest --config=jest.config.js -t 'electron'  --verbose ./tests/e2e/",
    "test-e2e-electron-minio": "node -r @babel/register ./internals/scripts/CheckBuiltsExist.js && cross-env HEADLESS_MODE=true jest --config=jest.config.minio.js -t 'minio' --verbose ./tests/e2e/",
    "test-e2e-web": "yarn prepare-web && cross-env HEADLESS_MODE=true jest --config=jest.config.web.js -t 'web' --verbose ./tests/e2e/",
    "test-e2e-web-all": "yarn prepare-web && jest --config=jest.config.web.js --verbose ./tests/e2e/",
    "minio-start": "./tests/bin/minio server ./tests/testdata/file-structure",
    "minio-start-windows": "./tests/bin/minio.exe server ./tests/testdata/file-structure",
    "start-chrome-driver": "./node_modules/.bin/chromedriver --url-base=/ --port=9515 --no-sandbox --disable-dev-shm-usage",
    "setup-tests": "node scripts/setup-tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tagspaces/tagspaces/"
  },
  "author": {
    "name": "TagSpaces UG (haftungsbeschraenkt)",
